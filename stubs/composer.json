{
    "name": "{{composer_id}}",
    "description": "{{description}}",
    "authors": [
        {
            "name": "{{author}}",
            "email": "{{email}}"
        }
    ],
    "require": {
        "php": ">=5.4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "4.*"
    },
    "autoload": {
        "psr-4": {
            "{{vendor}}\\{{name}}\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "{{vendor}}\\{{name}}\\Test\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
